<h2>Most Regular Sales</h2>
{{#if role  === 1}}
<table class="table">
	   <thead>
        
          <th>Product</th>
          <th>Quantity Sold</th>
          
      
      </thead>
      <tbody>
      	    {{#each mostSold}}
			<tr>
			    <td>{{product}}</td>
			    <td>{{soldNum}}</td>
			</tr>
		    {{/each}}
      </tbody>

</table>
{{else}}
	{{msg}}
{{/if}}

