
<h2>Categories</h2>
{{#if 1}}
<table class="table">

	   <thead>
        
          <th>Category</th>
          <th>Quantity Sold</th>
          
      
      </thead>
      
      	
      <tbody>
      	     {{#each categories}}
			<tr>
			    <td>{{category}}</td>
			    <td>{{quantity}}</td>
			</tr>
		    {{/each}}
      </tbody>
    {{else}}
      	{{msg}}
	{{/if}}

</table>
